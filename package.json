{
  "name": "@stavros-tsioulis/typed-events",
  "version": "1.0.0",
  "description": "A fully typed EventEmitter implementation compatible with Node.js EventEmitter API",
  "type": "module",
  "main": "./dist/events.js",
  "types": "./dist/events.d.ts",
  "module": "./dist/events.js",
  "exports": {
    ".": {
      "types": "./dist/events.d.ts",
      "import": "./dist/events.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "events",
    "eventemitter",
    "typed",
    "typescript",
    "node",
    "event",
    "emitter",
    "listener"
  ],
  "author": "Stavros Tsioulis",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/stavros-tsioulis/typed-events.git"
  },
  "bugs": {
    "url": "https://github.com/stavros-tsioulis/typed-events/issues"
  },
  "homepage": "https://github.com/stavros-tsioulis/typed-events#readme",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -p tsconfig.build.json",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "prepublishOnly": "pnpm build"
  },
  "devDependencies": {
    "typescript": "^5.6.2",
    "vite": "^5.4.6",
    "@types/node": "^20.16.5",
    "vitest": "^1.6.0"
  },
  "packageManager": "pnpm@9.7.0+sha512.dc09430156b427f5ecfc79888899e1c39d2d690f004be70e05230b72cb173d96839587545d09429b55ac3c429c801b4dc3c0e002f653830a420fa2dd4e3cf9cf"
